{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "Node",
    "paths": {
      "@/*": ["./src/*"],
      "@/api": ["./src/api"],
      "@/utils": ["./src/utils"]
    }
  }
}